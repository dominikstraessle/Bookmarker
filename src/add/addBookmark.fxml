<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.Label?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.Region?><?import javafx.scene.layout.VBox?><?import javafx.scene.text.Font?><?import com.jfoenix.controls.JFXTextField?><?import com.jfoenix.controls.JFXComboBox?><?import com.jfoenix.controls.JFXTextArea?><?import com.jfoenix.controls.JFXButton?>
<VBox prefHeight="300.0" prefWidth="500.0" spacing="10.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="add.AddBookmarkController">
   <children>
      <Label text="Add Bookmark">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <JFXTextField fx:id="addTxtUrl" promptText="URL" />
      <JFXTextField fx:id="addTxtTitle" promptText="Title" />
      <JFXTextField fx:id="addTxtTags" promptText="Tags" />
      <JFXComboBox fx:id="addComboEnv" promptText="Environment" />
      <JFXTextArea fx:id="addTxtDesc" promptText="Description" />
      <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
         <children>
            <Label fx:id="addLblAdded" text="Added: 12.12.12 05:36:10" />
            <Region HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="addBtnCancel" cancelButton="true" onAction="#handleCancel" styleClass="blue_light" text="Cancel">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="addBtnAdd" defaultButton="true" onAction="#handleAdd" styleClass="blue_dark" text="Add">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </JFXButton>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
